{
    "manifest_version": 3,
    "name": "Depo Scraper",
    "description": "Automates barcode search across multiple depo websites.",
    "version": "0.1",
    "permissions": [
      "tabs",
      "clipboardRead"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  