{
    "manifest_version": 3,
    "name": "Depo Scraper",
    "description": "Barkod aramalarını otomatikleştiren Chrome uzantısı.",
    "version": "1.0.0",
    "permissions": ["tabs"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
